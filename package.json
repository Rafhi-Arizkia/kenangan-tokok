{
  "name": "kenangan-commerce-backend",
  "version": "1.0.0",
  "description": "Commerce backend with Fastify, TypeScript, and Sequelize",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:create": "sequelize-cli db:create",
    "db:migrate": "NODE_OPTIONS='-r ts-node/register/transpile-only' sequelize-cli db:migrate",
    "db:migrate:undo": "NODE_OPTIONS='-r ts-node/register/transpile-only' sequelize-cli db:migrate:undo:all",
    "db:seed": "sequelize-cli db:seed:all"
  },
  "keywords": [
    "fastify",
    "typescript",
    "sequelize",
    "commerce",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.500.0",
    "@aws-sdk/lib-storage": "^3.500.0",
    "@fastify/cors": "^8.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/swagger": "^7.0.0",
    "@fastify/swagger-ui": "^1.3.0",
    "@scalar/fastify-api-reference": "^1.0.0",
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "fastify": "^4.24.3",
    "mysql2": "^3.6.5",
    "sequelize": "^6.35.0"
  },
  "optionalDependencies": {
    "@fastify/multipart": "^8.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/uuid": "^10.0.0",
    "sequelize-cli": "^6.6.3",
    "ts-node": "^10.9.1",
    "tsx": "^4.4.0",
    "typescript": "^5.2.2"
  }
}
